{
  "expo": {
    "name": "USBGF App",
    "slug": "ubgsf-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./app/assets/USBGF_com_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#1A1A2E"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.usbgf.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/icon.png",
        "backgroundColor": "#1A1A2E"
      },
      "package": "org.usbgf.mobile"
    },
    "extra": {
      "eas": {
        "projectId": "4d8f7596-6967-47e5-8fe1-056f2cf13724"
      }
    },
    "owner": "the_mastermind",
    "plugins": [
      "expo-font",
      "expo-document-picker",
      "expo-image-picker"
    ]
  }
}
